    "@vitest/coverage-v8": "^0.34.4",
    "@vitest/ui": "^0.34.4",
    "eslint": "^8.49.0",
    "tsup": "^7.2.0",
    "typescript": "^4.1.0",
    "vitest": "^0.34.4"
